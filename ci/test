docker-compose -f docker-compose.yaml -f docker-compose.build.yaml up --detach
docker-compose -f docker-compose.yaml -f docker-compose.build.yaml -f docker-compose.test.yaml build test-runner
docker-compose -f docker-compose.yaml -f docker-compose.build.yaml -f docker-compose.test.yaml run --rm test-runner
docker-compose -f docker-compose.yaml -f docker-compose.build
